<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="publicaciones_libros" language="groovy" pageWidth="802" pageHeight="555" orientation="Landscape" whenNoDataType="NoDataSection" columnWidth="802" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" uuid="4ffe69ed-61bd-400d-bd86-622907368a6e">
	<property name="ireport.zoom" value="2.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="anio" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="codigo" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT  UPPER(CASE P.PUB_PARTICIPACION WHEN 'I' THEN 'Individual'  WHEN 'P' THEN 'Capítulo en Libro Colectivo'
                ELSE 'N/I' END) AS PUB_PARTICIPACION,
UPPER(CASE P.PUB_ROL WHEN 1 THEN 'Autor' WHEN 2 THEN 'Coautor' WHEN 3 THEN 'Editor'
                WHEN 4 THEN 'Compilador' WHEN 5 THEN 'Revisor' WHEN 6 THEN 'Coordinador de Informe Periódico' ELSE 'N/I' END) AS PUB_ROL,
UPPER(CASE P.PUB_TIPOSVARIOS WHEN 1 THEN 'Académico' WHEN 2 THEN 'No Académico' ELSE 'N/I' END) AS PUB_TIPOSVARIOS,
UPPER(CASE P.PUB_TIPOEDICION WHEN 1 THEN 'Primera' WHEN 2 THEN 'Reedición' WHEN 3 THEN 'Recopilación' ELSE 'N/I' END) AS PUB_TIPOEDICION,
P.CAMPO10,
P.PUB_APELLIDO_AUTOR,
P.PUB_NOMBRE_AUTOR,
P.PUB_TITULO,
P.PUB_NOMBRE_CAPITULO,
P.PUB_EDITORIAL,
P.PUB_NUMPAGINAS,
CONVERT(CHAR(10),P.PUB_FECEDICION,103) AS PUB_FECEDICION,
P.PUB_NUMISBN,
UPPER(CASE P.AUSPICIO_PUBLICACION WHEN 'U' THEN 'UASB' WHEN 'O' THEN 'Otro' WHEN 'N' THEN 'N/A' ELSE 'N/I' END) AS AUSPICIO_PUBLICACION,
P.OTRO_AUSPICIO,
UPPER(CASE P.FORMA_PUBLICACION WHEN 'I' THEN 'Impreso' WHEN 'D' THEN 'Digital' ELSE 'N/I' END) AS FORMA_PUBLICACION,
(CASE P.PUB_PARTSERIE WHEN 0 THEN 'NO' WHEN 1 THEN 'SI' ELSE 'N/I' END) AS PUB_PARTSERIE,
P.PUB_TITSERIE,
(CASE P.PUB_REVPARES WHEN 'S' THEN 'SI' WHEN 'N' THEN 'NO' ELSE 'N/I' END) AS PUB_REVPARES,
P.PUB_URL,
(CASE P.PUB_ALCANCE WHEN 1 THEN 'NACIONAL' WHEN 2 THEN 'INTERNACIONAL' ELSE 'N/I' END) AS PUB_ALCANCE,
(SELECT dbo.PAIS.NOM_PAIS FROM PAIS WHERE dbo.PAIS.COD_PAIS = P.PAIS_AUSPICIO) PAIS,
(SELECT CIUDAD.NOM_CIUDAD  FROM CIUDAD WHERE COD_PAIS= P.PAIS_AUSPICIO AND COD_CIUDAD = P.CIUDAD_AUSPICIO) CIUDAD,
P.PUB_ROLANFITRION
FROM    PUBLICACION_PROFESOR P
WHERE   P.CODIGO_PROFESOR = $P{codigo}
AND P.TIPO_PUBLICACION = 13
AND P.CAMPO9 IS NULL
AND P.PUB_APELLIDO_AUTOR IS NOT NULL
AND P.CAMPO10 >= $P{anio}-1
AND P.PUB_PARTICIPACION <>'C'
ORDER BY P.CAMPO10 DESC]]>
	</queryString>
