	<queryString>
		<![CDATA[DECLARE @PROFESOR TABLE (CODIGO_PROFESOR NUMERIC(7,0),cod_profesor_acadant INT, nom_dedicacion varchar(15),NOM_TIPOCONTRATO VARCHAR(20) )
INSERT  INTO @PROFESOR(CODIGO_PROFESOR,cod_profesor_acadant,nom_dedicacion, NOM_TIPOCONTRATO)
SELECT CODIGO_PROFESOR, cod_profesor_acadant, nom_dedicacion, NOM_TIPOCONTRATO
FROM interfaseOcu.dbo.PROFESOR
DECLARE @VNUMESTMATERIA TABLE (ANIO NUMERIC(4,0), CODIGO_MATERIA NUMERIC(10,0),CODIGO_ESP NUMERIC(10,0),CODIGO_PARALELO NUMERIC(8,0), NUMEST NUMERIC(10,0), CODIGO_PROFESOR NUMERIC(8,0))
INSERT INTO @VNUMESTMATERIA(ANIO , CODIGO_MATERIA,CODIGO_ESP ,CODIGO_PARALELO, NUMEST , CODIGO_PROFESOR )
SELECT ANIO , CODIGO_MATERIA,CODIGO_ESP ,CODIGO_PARALELO , NUMEST , CODIGO_PROFESOR
FROM interfaseOcu.dbo.VISTA_NUMESTUDIANTE_MATERIA
WHERE ANIO = 2015
DECLARE @VCORDPROG TABLE (ANIO NUMERIC(38,0), CODIGO_PROFESOR NUMERIC(8,0), CODIGO_MATERIA NUMERIC(9,0), NOMBRE_MATERIA VARCHAR(100),
CODIGO_ESP VARCHAR(4), PROGRAMA VARCHAR(500), COD_PARALELO NUMERIC(8,0),CREDITO_DOCENTE NUMERIC(5,2), TRIMESTRE VARCHAR(75), NOMBRE_PARALELO VARCHAR(8),
NUMCLASE_PRACTICA NUMERIC(8,2), TIPO_ASIGNATURA VARCHAR(1))
INSERT INTO @VCORDPROG(ANIO , CODIGO_PROFESOR, CODIGO_MATERIA , NOMBRE_MATERIA ,
CODIGO_ESP , PROGRAMA , COD_PARALELO, CREDITO_DOCENTE, TRIMESTRE, NOMBRE_PARALELO, NUMCLASE_PRACTICA, TIPO_ASIGNATURA )
SELECT ANIO , CODIGO_PROFESOR, CODIGO_MATERIA , NOMBRE_MATERIA , CODIGO_ESP , PROGRAMA , COD_PARALELO,
CREDITO_DOCENTE, TRIMESTRE, NOMBRE_PARALELO, NUMCLASE_PRACTICA, TIPO_ASIGNATURA
FROM interfaseOcu.dbo.VISTA_COORDINADOR_PROGRAMA
WHERE  ANIO = 2015
SELECT (SELECT c.NOMBRE_CICLO
        FROM academico.dbo.CICLO_ACADEMICO c
        WHERE c.ANIO = e.anio) AS ANIO,
        (SELECT f.NOMBRE_FACULTAD
        FROM academico.dbo.FACULTAD f
        WHERE f.CODIGO_FACULTAD = e.CODIGO_FACULTAD) AS AREA,
e.NOMBRE_PROFESOR NOM_PROFESOR,
PROFESOR.NOM_TIPOCONTRATO,
PROFESOR.NOM_DEDICACION DEDICACION,
ISNULL(vcp.PROGRAMA, 'SIN ACTIVIDAD') AS PROGRAMA,
vcp.TRIMESTRE AS TRIMESTRE,
UPPER(vcp.NOMBRE_MATERIA + ' PARALELO ' + vcp.NOMBRE_PARALELO) AS NOMBRE_MATERIA,
(CASE WHEN vcp.CREDITO_DOCENTE=0 THEN vcp.NUMCLASE_PRACTICA ELSE CONVERT(INT,vcp.CREDITO_DOCENTE*10) END) NUM_CLASES,
vnm.NUMEST,
(CASE vcp.TIPO_ASIGNATURA WHEN 'T' THEN 'SI' ELSE (CASE WHEN vcp.TIPO_ASIGNATURA IS NULL THEN '' ELSE 'NO' END) END) TIPO_ASIGNATURA

FROM  interfaseOcu.GESTIONACADEMICA.ESTADISTICAS_INFORME_ANUAL e
INNER JOIN @PROFESOR PROFESOR ON PROFESOR.CODIGO_PROFESOR=e.CODIGO_PROFESOR
LEFT OUTER JOIN @VCORDPROG vcp  ON vcp.CODIGO_PROFESOR = e.CODIGO_PROFESOR
	AND vcp.ANIO = e.ANIO
LEFT OUTER  JOIN  @VNUMESTMATERIA vnm ON vnm.CODIGO_PROFESOR = vcp.CODIGO_PROFESOR
	AND vnm.ANIO = vcp.ANIO
	AND vnm.CODIGO_MATERIA = vcp.CODIGO_MATERIA
	AND vnm.CODIGO_ESP = vcp.CODIGO_ESP
	AND vnm.CODIGO_PARALELO = vcp.COD_PARALELO
WHERE   e.ANIO=$P{anio}
AND (CASE when ($P{tipReporte} = 'D' AND $P{codProfesor} IS NOT NULL) then  E.CODIGO_PROFESOR ELSE '0' END) = (CASE   when $P{tipReporte} = 'D' then $P{codProfesor} ELSE '0' END)
AND (CASE when(($P{tipReporte} ='A' OR $P{tipReporte} ='T' OR $P{tipReporte} ='E')and $P{codArea} <>'T')  then  E.CODIGO_FACULTAD ELSE '0' END )= (CASE  when(($P{tipReporte} ='A' OR $P{tipReporte} ='T' OR $P{tipReporte} ='E')and $P{codArea} <>'T')THEN $P{codArea}  ELSE '0' END)
AND (CASE when(($P{tipReporte}='T' OR $P{tipReporte} ='E')and $P{codTContrato} <> 'T') THEN E.COD_TIPOCONTRATO ELSE '0' END )= (CASE when(($P{tipReporte} ='T'OR $P{tipReporte} ='E')and $P{codTContrato} <> 'T')THEN  $P{codTContrato} ELSE '0' END  )
AND (CASE when($P{tipReporte} ='E'and $P{codDedicacion}<> 'T') THEN E.DEDICACION ELSE '0' END )= (CASE when($P{tipReporte} ='E'and $P{codDedicacion} <> 'T')THEN  $P{codDedicacion}ELSE '0' END  )]]>
	</queryString>
