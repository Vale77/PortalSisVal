	<queryString>
		<![CDATA[DECLARE @MATEST TABLE ( ANIO INT,CODIGO_ESP VARCHAR(4), NUM_EST INT)
DECLARE @MATERIA TABLE (AREA VARCHAR(75),   PROGRAMA VARCHAR(500), NIVEL_ACADEMICO VARCHAR(75),   CODIGO_ESP VARCHAR(4), CODIGO_FACULTAD NUMERIC(7,0),  ANIO INT, CODIGO_NIVEACAD VARCHAR(3),
                          CICLO VARCHAR(384))


INSERT INTO @MATEST  ( ANIO ,CODIGO_ESP, NUM_EST )
SELECT DISTINCT ANIO, CODIGO_ESP, NUMEST
FROM interfaseOcu.dbo.VISTA_NUMESTUDIANTE_PROGRAMA
INSERT INTO @MATERIA(AREA,  PROGRAMA,NIVEL_ACADEMICO,  CODIGO_ESP, CODIGO_FACULTAD,  ANIO, CODIGO_NIVEACAD,CICLO)
SELECT distinct  ARE_NOMBRE, NOMBRE_PROGRAMA ,TIPO_PROG, PRG_CODIGO , ARE_CODIGO,ANIO , COD_NIVEL_ACAD, 1
FROM interfaseOcu.dbo.PROGRAMA
select distinct CICLO.NOMBRE_CICLO,MATERIA.AREA, MATERIA.programa, MATERIA.nivel_academico, VEVALCONSOL.anio,CODIGO_NIVEACAD,
VEVALCONSOL.codigo_pregunta, VEVALCONSOL.pregunta, VEVALCONSOL.valor, MATEST.NUM_EST  na,VEVALCONSOL.ne2010
from  @MATERIA AS MATERIA LEFT OUTER JOIN interfaseOcu.EVALUACION.f_vista_tabulacion_evaldocentefecha($P{fecInicio},$P{fecFin}) AS VEVALCONSOL
ON VEVALCONSOL.ANIO = MATERIA.ANIO
AND VEVALCONSOL.CODIGO_ESP = MATERIA.CODIGO_ESP INNER JOIN @MATEST AS MATEST
ON MATERIA.ANIO = MATEST.ANIO
AND MATERIA.CODIGO_ESP = MATEST.CODIGO_ESP INNER JOIN interfaseOcu.dbo.CICLO_ACADEMICO AS CICLO
ON VEVALCONSOL.ANIO = CICLO.ANIO
where VEVALCONSOL.TIPO = 'P'
and VEVALCONSOL.CODIGO_MATERIA = -1
and VEVALCONSOL.CODIGO_PROFESOR = -1
and VEVALCONSOL.codigo_nivel = -1
ORDER BY AREA, CODIGO_NIVEACAD DESC, PROGRAMA]]>
