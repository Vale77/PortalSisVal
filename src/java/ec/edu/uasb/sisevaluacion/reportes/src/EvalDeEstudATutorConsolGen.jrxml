VEVALCONSOL.na,VEVALCONSOL.ne2010, VEVALCONSOL.NOMBRE_PROFESOR
 from interfaseOcu.EVALUACION.VISTA_EVALESTUDATUTOR_CONSOL AS VEVALCONSOL INNER JOIN interfaseOcu.dbo.CICLO_ACADEMICO AS CICLO
 ON VEVALCONSOL.ANIO_EVAL = CICLO.ANIO
