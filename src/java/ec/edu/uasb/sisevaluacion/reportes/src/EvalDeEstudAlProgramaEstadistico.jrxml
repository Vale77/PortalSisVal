	<queryString>
		<![CDATA[DECLARE @MATERIA TABLE ( AREA VARCHAR(75), PROGRAMA VARCHAR(500),CODIGO_ESP VARCHAR(4), CODIGO_FACULTAD NUMERIC(7,0),  ANIO INT, CODIGO_NIVEACAD VARCHAR(3), CICLO VARCHAR(384))
DECLARE @MATEST TABLE ( ANIO INT,CODIGO_ESP VARCHAR(4), NUM_EST INT)
INSERT INTO @MATERIA(AREA, PROGRAMA ,CODIGO_ESP , CODIGO_FACULTAD ,  ANIO , CODIGO_NIVEACAD , CICLO )
SELECT distinct  ARE_NOMBRE, NOMBRE_PROGRAMA ,PRG_CODIGO , ARE_CODIGO,ANIO , COD_NIVEL_ACAD , 1
FROM interfaseOcu.dbo.PROGRAMA with (nolock)
where anio = $P{anio}
INSERT INTO @MATEST  ( ANIO ,CODIGO_ESP, NUM_EST )
SELECT ANIO, CODIGO_ESP, NUMEST
FROM interfaseOcu.dbo.VISTA_NUMESTUDIANTE_PROGRAMA with (nolock)
WHERE ANIO = $P{anio}
select DISTINCT  ISNULL(ENCCAL.ANIO,MATERIA.ANIO ) AS ANIO,
ENCCAL.FECHA_FIN AS FINEVAL,
ISNULL(MATEST.NUM_EST,1) as totenc,
ISNULL((SELECT COUNT(*)
FROM interfaseOcu.EVALUACION.ENCUESTA_REALIZADA AS ENCRE with (nolock) LEFT JOIN interfaseOcu.dbo.VISTA_ESTUDIANTES_BLOQUEADOS ESTBLO  with (nolock)
ON ENCRE.ANIO= ESTBLO.ANIO
AND ENCRE.CODIGO_ALUMNO = ESTBLO.CODIGO_ESTUDIANTE
AND ENCRE.CODIGO_ESP = ESTBLO.CODIGO_ESP
WHERE ESTBLO.CODIGO_ESTUDIANTE IS NULL
AND ENCRE.ANIO = MATERIA.ANIO
AND ENCRE.CODIGO_ESP = MATERIA.CODIGO_ESP
AND ENCRE.CODIGO_NIVEL = -1
AND ENCRE.CODIGO_MATERIA = -1
AND ENCRE.CODIGO_PROFESOR = -1
and ENCRE.codigo_encuesta = ENCCAL.CODIGO_ENCUESTA),0 )AS TOTENCRE,
ENCCAL.ESTADO_ENCUESTA,
MATERIA.AREA,
MATERIA.PROGRAMA,
(case ENCCAL.estado_encuesta when 'A' then 'Abierta' when 'C' then 'Cerrada' end) estado,
MATERIA.CODIGO_NIVEACAD, NOMBRE_CICLO
from @MATERIA AS MATERIA LEFT OUTER JOIN interfaseOcu.EVALUACION.ENCUESTA_CALENDARIO AS ENCCAL with (nolock)
ON MATERIA.ANIO = ENCCAL.ANIO
AND MATERIA.CODIGO_ESP = ENCCAL.CODIGO_ESP INNER JOIN @MATEST AS MATEST
ON MATERIA.ANIO  = MATEST.ANIO
AND MATERIA.CODIGO_ESP = MATEST.CODIGO_ESP INNER JOIN interfaseOcu.dbo.CICLO_ACADEMICO with (nolock)
ON MATERIA.ANIO = CICLO_ACADEMICO.ANIO
WHERE MATERIA.ANIO = $P{anio}
AND ENCCAL.TIPO_ENCUESTA ='P'
and ENCCAL.CODIGO_MATERIA = -1
and ENCCAL.CODIGO_PROFESOR = -1
and ENCCAL.CODIGO_NIVEL = -1
AND (case $P{estado} when 'T' then '0' else ENCCAL.estado_encuesta end ) = (case $P{estado} when 'T' then '0' else $P{estado} end )
AND (CASE when $P{codArea} <>'T'  then  MATERIA.CODIGO_FACULTAD ELSE '0' END )= (CASE  when $P{codArea} <>'T'THEN $P{codArea} ELSE '0' END)]]>
	</queryString>
